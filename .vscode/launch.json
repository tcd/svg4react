{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "dev",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--nolazy",
            ],
            "args": [
                "./build/tasks/docs/build-prop-docs.js",
            ],
            "cwd": "${workspaceRoot}/packages/dev",
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ]
        }
    ]
}
